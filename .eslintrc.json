{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": ["react", "jsx-a11y", "import", "better-styled-components"],
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [
            "error",
            {
                "extensions": [".js",".jsx"]
            }
        ],
        "import/prefer-default-export": "off"
    },
    "settings": {
        "import/resolver": {
            "node": {
                "paths": ["src"]
            }
        }
    }
}